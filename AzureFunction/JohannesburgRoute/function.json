{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "mytimer",
      "type": "timerTrigger",
      "direction": "in",
      "schedule": "0 0 */1 * * *"
    },
    {
      "type": "blob",
      "name": "outputBlob",
      "path": "johannesburgroutes/{DateTime}.csv",
      "direction": "out",
      "connection": "routestorage"
    }
  ],
  "disabled": false
}
